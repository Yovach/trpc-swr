{
  "name": "trpc-swr",
  "version": "0.0.0",
  "type": "module",
  "description": "tRPC-ified SWR hooks",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./infinite": {
      "import": "./dist/infinite.js",
      "default": "./dist/infinite.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "peerDependencies": {
    "@trpc/client": ">=9.23.2",
    "@trpc/server": ">=9.23.2",
    "react": "^17 || ^18",
    "swr": "^1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@trpc/client": "npm:@sachinraja/trpc-client@10.0.0-fetch.0",
    "@trpc/server": "npm:@sachinraja/trpc-server@10.0.0-fetch.0",
    "@types/node": "17.0.33",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "@vitejs/plugin-react": "1.3.2",
    "dprint": "0.28.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "get-port": "6.1.2",
    "husky": "8.0.1",
    "jsdom": "19.0.0",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "react": "18.1.0",
    "swr": "1.3.0",
    "tsup": "5.12.7",
    "typescript": "4.6.4",
    "undici": "5.2.0",
    "vite": "2.9.9",
    "vitest": "0.12.6",
    "zod": "3.16.0"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "tsup": {
    "entry": [
      "src/index.tsx",
      "src/infinite.ts"
    ],
    "target": "node14",
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  }
}
